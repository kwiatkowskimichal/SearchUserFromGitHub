<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SearchUserFromGitHub - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")

</head>
<body  ng-app="GIT_SHOW_REPO">
    <div class="container body-content">
        @RenderBody()
        <hr />
